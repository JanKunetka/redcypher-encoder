<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <OutputType>WinExe</OutputType>
        <TargetFramework>net6.0-windows</TargetFramework>
        <Nullable>enable</Nullable>
        <UseWPF>true</UseWPF>
        <RootNamespace>View</RootNamespace>
        <ApplicationIcon>Resource\img\img_Icon.ico</ApplicationIcon>
    </PropertyGroup>

    <ItemGroup>
      <Folder Include="Resource\img" />
    </ItemGroup>

    <ItemGroup>
      <None Remove="Resource\img\img_AddFile.png" />
      <Resource Include="Resource\img\img_AddFile.png" />
      <None Remove="Resource\img\img_Icon.png" />
      <Resource Include="Resource\img\img_Icon.png" />
      <None Remove="Resource\img\img_Icon.ico" />
      <Resource Include="Resource\img\img_Icon.ico" />
    </ItemGroup>

    <ItemGroup>
      <ProjectReference Include="..\ViewModels\ViewModels.csproj" />
    </ItemGroup>

</Project>
